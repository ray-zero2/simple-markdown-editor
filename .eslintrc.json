{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 12
  },
  "globals": {
  },
  "rules": {
    "no-console"  : 0,
    "no-alert"    : 0,
    "no-debugger" : 1
  }
}
